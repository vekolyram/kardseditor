<adonisControls:AdonisWindow  
    xmlns:adonisControls="clr-namespace:AdonisUI.Controls;assembly=AdonisUI"
    x:Class="kardseditor.lookup"
            xmlns:adonisUi="clr-namespace:AdonisUI;assembly=AdonisUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kardseditor"
        mc:Ignorable="d"
        Title="lookup" Height="275" Width="314">
    <DockPanel Margin="10">
        <TextBox x:Name="t1" TextWrapping="Wrap" Text="Index" Width="NaN" DockPanel.Dock="Top" KeyDown="t1_KeyDown"/>
        <TextBox x:Name="l1"  Width="NaN" Height="NaN" DockPanel.Dock="Top" FontWeight="Normal" FontSize="13" Text="Info" IsReadOnly="True" Margin="0,10,0,0"/>
        <Button Content="Find" DockPanel.Dock="Bottom" Height="30" Click="Button_Click" VerticalAlignment="Bottom" Style="{DynamicResource {x:Static adonisUi:Styles.AccentButton} }"/>
    </DockPanel>
</adonisControls:AdonisWindow>
